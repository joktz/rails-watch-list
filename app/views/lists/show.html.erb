<div class="container">
  <%= @list.name %>
  <ul>
    <% @list.movies.each do |movie| %>
      <li class="container">
        <img src="<%= movie.poster_url %>"/>
        <%= movie.title %>
        <%= movie.overview %>
      </li>
    <% end %>
  </ul>
  <%= link_to "Add movie", new_list_bookmark_path(@list), class: "btn btn-primary" %>
</div>
